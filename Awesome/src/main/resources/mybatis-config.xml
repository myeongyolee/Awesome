<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <settings>
  	<setting name="jdbcTypeForNull" value="NULL"/>
  	<!-- 파라미터전달된 데이터가 null인 경우, db에서도 null로 인식해라 -->
  	<setting name="mapUnderscoreToCamelCase" value="true"/>
  	<!-- 컬럼명에_를 사용한 경우, java코드에서는 camelCasing으로 치환할 것| -->
  </settings>

  <typeAliases>
	<typeAlias type="com.kh.awesome.matchManager.model.vo.MatchManager" alias="matchManager"/>
	<typeAlias type="com.kh.awesome.club.model.vo.Club" alias="club"/>
	<typeAlias type="com.kh.awesome.club.model.vo.Clubmember" alias="clubmember"/>
	<typeAlias type="com.kh.awesome.club.model.vo.Clubcontent" alias="clubcontent"/>
	<typeAlias type="com.kh.awesome.board.model.vo.QuestionBoard" alias="qBoard"/>
	<typeAlias type="com.kh.awesome.common.typeHandler.StringToLongTypeHandler" alias="StringToLongTypeHandler"/>
	<typeAlias type="com.kh.awesome.member.model.vo.Member" alias="member"/>
	<typeAlias type="com.kh.awesome.member.model.vo.Address" alias="address"/>
	<typeAlias type="com.kh.awesome.socket.stomp.model.vo.Msg" alias="msg"/>
	<typeAlias type="com.kh.awesome.socket.stomp.model.vo.ChatRoom" alias="chatRoom"/>
	<typeAlias type="com.kh.awesome.util.model.vo.Chat" alias="chatting"/>
  </typeAliases> 

  
<!--   <typeHandlers>
	<typeHandler handler="com.kh.awesome.common.handler.CustomDateHandler"/>
	</typeHandlers> -->

</configuration>