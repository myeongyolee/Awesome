<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <settings>
  	<setting name="jdbcTypeForNull" value="NULL"/>
  	<!-- 파라미터전달된 데이터가 null인 경우, db에서도 null로 인식해라 -->
  	<setting name="mapUnderscoreToCamelCase" value="true"/>
  	<!-- 컬럼명에_를 사용한 경우, java코드에서는 camelCasing으로 치환할 것| -->
  </settings>
  
  <typeAliases>

	<typeAlias type="com.kh.awesome.matchManager.model.vo.MatchManager" alias="matchManager"/>

	<typeAlias type="com.kh.awesome.club.model.vo.Club" alias="club"/>

  </typeAliases>
  
	<typeHandlers>
		<typeHandler handler="com.kh.awesome.common.typeHandler.StringToLongTypeHandler" javaType="String" 
					jdbcType="LONGVARCHAR"/>
	</typeHandlers>
  
</configuration>