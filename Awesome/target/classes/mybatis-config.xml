<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <settings>
  	<setting name="jdbcTypeForNull" value="NULL"/>
  	<!-- 파라미터전달된 데이터가 null인 경우, db에서도 null로 인식해라 -->
  	<setting name="mapUnderscoreToCamelCase" value="true"/>
  	<!-- 컬럼명에_를 사용한 경우, java코드에서는 camelCasing으로 치환할 것| -->
  </settings>
  
<typeAliases>
<typeAlias alias="matchManager" type="com.kh.awesome.matchManager.model.vo.MatchManager"/>
<typeAlias alias="club" type="com.kh.awesome.club.model.vo.Club"/>
<typeAlias alias="clubmember" type="com.kh.awesome.club.model.vo.Clubmember"/>
<typeAlias alias="clubcontent" type="com.kh.awesome.club.model.vo.Clubcontent"/>
<typeAlias alias="clubphoto" type="com.kh.awesome.club.model.vo.Clubphoto"/>
<typeAlias alias="clubcomment" type="com.kh.awesome.club.model.vo.Clubcomment"/>
<typeAlias alias="schoolClub" type="com.kh.awesome.school.model.vo.SchoolClub"/>
<!-- <typeAlias alias="qBoard" type="com.kh.awesome.board.model.vo.QuestionBoard"/> -->
<typeAlias type="com.kh.awesome.member.model.vo.Member" alias="member"/>
<typeAlias type="com.kh.awesome.member.model.vo.Address" alias="address"/>
<!-- <typeAlias type="com.kh.awesome.admin.model.vo.Report" alias="report"/> -->
<!-- <typeAlias type="com.kh.awesome.socket.stomp.model.vo.Msg" alias="msg"/> -->
<!-- <typeAlias type="com.kh.awesome.socket.stomp.model.vo.ChatRoom" alias="chatRoom"/> -->
<typeAlias alias="StringToLongTypeHandler" type="com.kh.awesome.common.typeHandler.StringToLongTypeHandler"/>
</typeAliases>

<typeHandlers>
<typeHandler jdbcType="LONGVARCHAR" javaType="String" handler="com.kh.awesome.common.typeHandler.StringToLongTypeHandler"/>
</typeHandlers>
</configuration>